{
  "name": "jeffreyemanuel-site",
  "version": "0.2.0",
  "private": true,
  "packageManager": "bun@1.3.3",
  "engines": {
    "npm": "DO NOT USE NPM - Use bun instead",
    "bun": ">=1.3.0"
  },
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('npm') !== -1) { console.error('\\n\\n‚ùå ERROR: This project uses BUN, not npm!\\n\\nPlease use: bun install\\n\\n'); process.exit(1); }\"",
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "build:with-stats": "bun run update-stats && rm -rf .next && next build",
    "update-stats": "bun run scripts/update-stats.ts",
    "fetch-tldr-stars": "bun run scripts/fetch-tldr-stars.ts",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@octokit/rest": "^22.0.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/d3": "^7.4.3",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "feed": "^5.2.0",
    "framer-motion": "^12.34.0",
    "fuse.js": "^7.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.14.2",
    "katex": "^0.16.28",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "postprocessing": "^6.38.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/katex": "^0.16.8",
    "@types/minimatch": "^6.0.0",
    "@types/node": "25.2.2",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "0.182.0",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "trustedDependencies": [
    "unrs-resolver"
  ]
}
